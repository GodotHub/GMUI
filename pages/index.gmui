<Row align="center">
	<Column align="center">
		<Button text="Hello World" ref="my_button"></Button>
	</Column>
</Row>

<Script>
func _mounted():
	self.set_anchors_and_offsets_preset(Control.PRESET_FULL_RECT)
	gmui.refs['my_button'].rnode.pressed.connect(
		func():
			print(gmui.refs['my_button'].rnode.text)
	)
</Script>

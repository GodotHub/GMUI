<Row align="center">
	<Label :text="fullName"></Label>
</Row>

<Script>
var data = await reactive({'firstName': 'zhang', 'lastName': 'san'})

func _ready():
	computed(fullName)

func fullName():
	return data.rget('firstName') + data.rget('lastName')

func _mounted():
	data.rset('firstName', 'li')

</Script>
